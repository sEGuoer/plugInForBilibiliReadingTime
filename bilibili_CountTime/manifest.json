{
  "manifest_version": 3,
  "name": "B站多功能插件",
  "version": "0.0.2",
  "description": "适用于B站调整倍速与合集统计时间",
  "icons": {
    "16": "img/favicon16.png",
    "32": "img/favicon32.png",
    "64": "img/favicon64.png",
    "128": "img/favicon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "https://www.bilibili.com/video/*"
      ]
    }
  ]
}
